      <link name="wheel_left_link">
        <pose>0 0.3493 0 -1.5707 0 0</pose>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.03</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.05</iyy>
            <iyz>0</iyz>
            <izz>0.03</izz>
          </inertia>
          <mass>1</mass>
        </inertial>
        <visual name="wheel_left_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1082</radius>
              <length>0.056</length>
            </cylinder>
          </geometry>
