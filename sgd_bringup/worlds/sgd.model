<?xml version="1.0"?>
<sdf version="1.6">
  <world name="default">

    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose frame=''>0.319654 -0.235002 9.29441 0 1.5138 0.009599</pose>
        <view_controller>orbit</view_controller>
        <projection_type>perspective</projection_type>
      </camera>
    </gui>

    <physics type="ode">
      <real_time_update_rate>1000.0</real_time_update_rate>
      <min_step_size>0.00001</min_step_size>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <ode>
        <solver>
          <type>world</type>
          <!--iters>150</iters>
          <precon_iters>0</precon_iters>
          <sor>1.400000</sor-->
          <use_dynamic_moi_rescaling>1</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0.00001</cfm>
          <erp>0.2</erp>
          <!--contact_max_correcting_vel>2000.000000</contact_max_correcting_vel-->
          <contact_max_correcting_vel>20</contact_max_correcting_vel>
          <contact_surface_layer>0.000100</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
    <gravity>0 0 -9.81</gravity>
    
    <spherical_coordinates>
      <surface_model>EARTH_WGS84</surface_model>
      <latitude_deg>10.021944</latitude_deg>
      <longitude_deg>53.555833</longitude_deg>
      <elevation>6.0</elevation>
    </spherical_coordinates>

    
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <scene>
      <shadows>false</shadows>
    </scene>

    <model name="test_world">
      <static>1</static>
      <include>
        <uri>model://test_world</uri>
      </include>
    </model>

    <!--model name="blindenhund">
      <pose>-2.0 -0.5 0.01 0 0 0</pose>
      <include>
        <uri>model://blindenhund</uri>
      </include>
    </model-->

    <model name="blindenhund">
      <pose>-2.0 -0.5 0.15 0.0 0.0 0.0</pose>
      <link name="base_footprint"/>

      <link name="base_link">
        <pose>0.3576 0 0.1524 0 0 0</pose>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <mass>8</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.1</izz>
          </inertia>
        </inertial>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
        <visual name="base_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.7152 0.628 0.20</size>
            </box>
          </geometry>
        </visual>
        <collision name="base_collision">
          <max_contacts>10</max_contacts>
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.7152 0.628 0.20</size>
            </box>
          </geometry>
        </collision>        
      </link>

      <link name="imu_link">
        <pose>0 0 0 0 0 0</pose>
        <sensor name="sgd_imu" type="imu">
          <always_on>true</always_on>
          <update_rate>200</update_rate>
          <imu>
            <angular_velocity>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </z>
            </angular_velocity>
            <linear_acceleration>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </z>
            </linear_acceleration>
          </imu>
          <plugin name="blindenhund_imu" filename="libgazebo_ros_imu_sensor.so">
            <ros>
              <remapping>~/out:=imu</remapping>
            </ros>
          </plugin>
        </sensor>
        <self_collide>0</self_collide>
        <inertial>
          <pose>0 0 0 0 -0 0</pose>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>0.001</mass>
        </inertial>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
      </link>

      <link name="base_scan">
        <pose>0.6 -0 0.3 0 -0 0</pose>
        <self_collide>0</self_collide>
        <enable_wind>0</enable_wind>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.2</mass>
        </inertial>
        <visual name="lidar_sensor_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>model://blindenhund/meshes/lds.dae</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
        </visual>
        <collision name="lidar_sensor_collision">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.0508</radius>
              <length>0.055</length>
            </cylinder>
          </geometry>
        </collision>

        <sensor name="sick_tim5xx" type="ray">
          <always_on>true</always_on>
          <visualize>true</visualize>
          <pose>0 0 0 0 0 0</pose>
          <update_rate>15</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>810</samples>
                <resolution>0.33</resolution>
                <min_angle>-2.3562</min_angle>
                <max_angle>2.3562</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.15</min>
              <max>25</max>
              <resolution>0.06</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.01</stddev>
            </noise>
          </ray>
          <plugin name="sick_laserscan" filename="libgazebo_ros_ray_sensor.so">
            <ros>
                <remapping>~/out:=scan</remapping>
            </ros>
            <output_type>sensor_msgs/LaserScan</output_type>
            <frame_name>base_scan</frame_name>
          </plugin>
        </sensor>
      </link>

      <link name="wheel_left_link">
        <pose>0 0.3493 0 -1.5707 0 0</pose>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>14.5833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>14.5833</iyy>
            <iyz>0</iyz>
            <izz>12.5</izz>
          </inertia>
          <mass>2</mass>
        </inertial>
        <visual name="wheel_left_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1524</radius>
              <length>0.056</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="wheel_left_collision">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1524</radius>
              <length>0.056</length>
            </cylinder>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>1.0</mu2>
                <fdir1>0 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
            </friction>
            <contact>
              <ode>
                <!--soft_cfm>0</soft_cfm>
                <soft_erp>0.2</soft_erp-->
                <kp>1e+5</kp>
                <kd>1</kd>
                <max_vel>0.1</max_vel>
                <min_depth>0.01</min_depth>
              </ode>
            </contact>
          </surface>
        </collision>
      </link>

      <link name="wheel_right_link">
        <pose>0 -0.3493 0 -1.5707 0 0</pose>
        <inertial>
          <pose>0 0 0 0 -0 0</pose>
          <inertia>
            <ixx>14.5833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>14.5833</iyy>
            <iyz>0</iyz>
            <izz>12.5</izz>
          </inertia>
          <mass>2</mass>
        </inertial>
        <visual name="wheel_right_visual">
          <pose>0 0 0 0 -0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1524</radius>
              <length>0.056</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="wheel_right_collision">
          <pose>0 0 0 0 -0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.1524</radius>
              <length>0.056</length>
            </cylinder>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>1.0</mu2>
                <fdir1>0 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
            </friction>
            <contact>
              <ode>
                <!--soft_cfm>0</soft_cfm>
                <soft_erp>0.2</soft_erp-->
                <kp>1e+5</kp>
                <kd>1</kd>
                <max_vel>0.1</max_vel>
                <min_depth>0.01</min_depth>
              </ode>
            </contact>
          </surface>
        </collision>
      </link>

      <link name='caster_link'>
        <pose>0.7152 0 0 0 0 0</pose>
        <inertial>
          <mass>3</mass>
          <inertia>
            <ixx>1.0</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>1.0</iyy>
            <iyz>0.000</iyz>
            <izz>1.0</izz>
          </inertia>
          <pose>0 0 0 0 -0 0</pose>
        </inertial>
        <visual name="visual">
          <geometry>
            <sphere>
              <radius>0.1524</radius>
            </sphere>
          </geometry>
          <pose>0 0 0 0 0 0</pose>
        </visual>
        <collision name='collision'>
          <max_contacts>10</max_contacts>
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>0.1524</radius>
            </sphere>
          </geometry>
          <surface>
            <contact>
              <ode>
                <!--soft_cfm>0</soft_cfm>
                <soft_erp>0.2</soft_erp-->
                <kp>1e+5</kp>
                <kd>1</kd>
                <max_vel>0.1</max_vel>
                <min_depth>0.01</min_depth>
              </ode>
            </contact>
          </surface>
        </collision>
      </link>

      <link name="gps_link">
        <pose>0.3 0 0.3 0 0 0</pose>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.1</mass>
        </inertial>
        <visual name="gps_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 0.05 0.05</size>
            </box>
          </geometry>
        </visual>
        <collision name="gps_collision">
          <max_contacts>10</max_contacts>
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 0.05 0.05</size>
            </box>
          </geometry>
        </collision>
        <sensor name="gps_sensor" type="gps">
          <always_on>true</always_on>
          <update_rate>1</update_rate>
          <gps>
            <position_sensing>
              <horizontal>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </horizontal>
              <vertical>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </vertical>
            </position_sensing>
          </gps>
          <plugin name="my_gps_plugin" filename="libgazebo_ros_gps_sensor.so">
            <ros>
              <remapping>~/out:=gps</remapping>
            </ros>
            <!--output_type>sensor_msgs/LaserScan</output_type-->
            <frame_name>map</frame_name>
          </plugin>
        </sensor>
      </link>
      
      <link name="camera_link">
        <pose>0.75 -0.065 0.2 0 -0 0</pose>
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.035</mass>
        </inertial>
        <collision name="collision">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.008 0.130 0.022</size>
            </box>
          </geometry>
        </collision>
        <sensor name="intel_realsense_r200_depth" type="depth">
          <always_on>1</always_on>
          <update_rate>5</update_rate>
          <pose>0 0 0 0 0 0</pose>
          <camera name="realsense_depth_camera">
          </camera>
          <plugin name="intel_realsense_r200_depth_driver" filename="libgazebo_ros_camera.so">
            <ros>
            </ros>
            <camera_name>intel_realsense_r200_depth</camera_name>
            <frame_name>camera_depth_frame</frame_name>
            <hack_baseline>0.07</hack_baseline>
            <min_depth>0.001</min_depth>
            <max_depth>5.0</max_depth>
          </plugin>
        </sensor>
      </link>

      <joint name="base_joint" type="fixed">
        <parent>base_footprint</parent>
        <child>base_link</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <joint name="wheel_left_joint" type="revolute">
        <parent>base_link</parent>
        <child>wheel_left_link</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
      </joint>

      <joint name="wheel_right_joint" type="revolute">
        <parent>base_link</parent>
        <child>wheel_right_link</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
      </joint>

      <joint name='caster_joint' type='ball'>
        <parent>base_link</parent>
        <child>caster_link</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <joint name="lidar_joint" type="fixed">
        <parent>base_link</parent>
        <child>base_scan</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <joint name="camera_joint" type="fixed">
        <parent>base_link</parent>
        <child>camera_link</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <joint name="gps_joint" type="fixed">
        <parent>base_link</parent>
        <child>gps_link</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <joint name="camera_rgb_joint" type="fixed">
        <parent>camera_link</parent>
        <child>camera_rgb_frame</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <plugin name="turtlebot3_diff_drive" filename="libgazebo_ros_diff_drive.so">
        <update_rate>30</update_rate>

        <left_joint>wheel_left_joint</left_joint>
        <right_joint>wheel_right_joint</right_joint>

        <wheel_separation>0.648</wheel_separation>
        <wheel_diameter>0.3048</wheel_diameter>

        <max_wheel_torque>20</max_wheel_torque>
        <max_wheel_acceleration>0.2</max_wheel_acceleration>

        <command_topic>cmd_vel</command_topic>

        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>false</publish_wheel_tf>

        <odometry_topic>odom</odometry_topic>
        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>base_footprint</robot_base_frame>
      </plugin>

      <plugin name="turtlebot3_joint_state" filename="libgazebo_ros_joint_state_publisher.so">
        <ros>
          <remapping>~/out:=joint_states</remapping>
        </ros>
        <update_rate>30</update_rate>
        <joint_name>wheel_left_joint</joint_name>
        <joint_name>wheel_right_joint</joint_name>
      </plugin>

    </model>

  </world>
</sdf>
